{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "env": ["*"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "env": ["*"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    }
  }
}
